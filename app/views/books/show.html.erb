<div class="main-container-content">

  <div class="flex-box-book-show">
      <div class="col rounded p-3">
        <h1>Review of <%= @book.title %></h1>
        <h2>Reviews <%= @review.content %></h2>
        <h2>Rating <%= @review.rating %>
        <h2>Your favorites <%= @review.top %></h2>
      </div>
  </div>

  <%= link_to "Add a review", new_book_review_path(@book), class: "btn btn-book"  %>

</div>
